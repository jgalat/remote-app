{
  "name": "@remote-app/transmission-client",
  "version": "0.1.4",
  "description": "Transmission RPC client",
  "keywords": [
    "transmission",
    "rpc",
    "client"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgalat/remote-app.git",
    "directory": "packages/transmission-client"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "exports": {
    ".": {
      "react-native": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./schemas": {
      "react-native": "./src/schemas.ts",
      "import": {
        "types": "./dist/schemas.d.mts",
        "default": "./dist/schemas.mjs"
      },
      "require": {
        "types": "./dist/schemas.d.cts",
        "default": "./dist/schemas.cjs"
      }
    }
  },
  "react-native": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "test": "vitest run"
  },
  "dependencies": {
    "base-64": "^1.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/base-64": "^1.0.0"
  }
}
